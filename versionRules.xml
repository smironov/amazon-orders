<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <rules>
        <rule groupId="org.apache.pdfbox" artifactId="pdfbox">
            <ignoreVersions>
                <ignoreVersion type="regex">3\.0\.\d+(-(alpha|beta|RC)\d+)?</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.apache.maven.plugins">
            <ignoreVersions>
                <ignoreVersion type="regex">\d+\.\d+\.\d+-beta-\d+</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
